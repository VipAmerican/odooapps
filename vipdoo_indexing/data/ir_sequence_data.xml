<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for customer -->
        <record id="seq_base_customer" model="ir.sequence">
            <field name="name">Customer</field>
            <field name="code">res.customer</field>
            <field name="prefix">C</field>
            <field name="padding">7</field>
            <field name="number_next">2000001</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for vendor -->
        <record id="seq_base_vendor" model="ir.sequence">
            <field name="name">Vendor</field>
            <field name="code">res.vendor</field>
            <field name="prefix">V</field>
            <field name="padding">7</field>
            <field name="number_next">6000001</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
